#include <stdio.h>
#include <math.h>
int main() {
   int n,copy,sum=0,digits=0;
   scanf("%d",&n);
   copy=n;
   while(copy>0){
       copy=copy/10;
       digits++;
   }
   copy=n;
   while(copy>0){
       sum=sum+ (int)pow(copy%10,digits);
       copy=copy/10;
   }
   if(sum==n){
       printf("Armstrong number");
   }
   else{
       printf("Not an Armstrong number");
   }
}
